  "license": "ISC",
  "bugs": {
    "url": "https://github.com/daru23/react-context-theme/issues"
  },
  "homepage": "https://github.com/daru23/react-context-theme#readme",
  "dependencies": {
    "@babel/polyfill": "^7.0.0-beta.51",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.11",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.4.0",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "webpack": "^4.12.2",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4"
