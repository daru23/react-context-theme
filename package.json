{
  "name": "react-context-theme",
  "version": "1.0.0",
  "description": "React Context API to handle theme",
  "main": "index.js",
  "scripts": {
    "start": "node node_modules/webpack-dev-server/bin/webpack-dev-server --mode development --open",
    "build": "node ./node_modules/webpack/bin/webpack --mode development",
    "dev-noconfig": "node ./node_modules/webpack/bin/webpack --mode development --module-bind js=babel-loader",
    "build-noconfig": "node ./node_modules/webpack/bin/webpack --mode production --module-bind js=babel-loader"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daru23/react-context-theme.git"
  },
  "keywords": [
    "react",
